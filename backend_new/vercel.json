{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "use": "@vercel/node"
  },
  "buildCommand": "npm install --production=false"
}